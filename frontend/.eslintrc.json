{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": "../.eslintrc.json",
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        },
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-vars": 1,
        "no-console": 0,
        "no-irregular-whitespace": 0

    }
}